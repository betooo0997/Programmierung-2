// Consolenfenster

using System;

class Program
{
    static void Main()
    {
	Console.Write("Zeile 1 ");    
	Console.Write("Zeile1 ");     
	Console.Write("Zeile 1");    
	Console.WriteLine();       

	Console.WriteLine("Zeile 2"); 
    }
}
// oder auch das hello world beispiel:
 
 using System;
namespace HelloWorld
{
    class Hello 
    {
        static void Main() 
        {
            Console.WriteLine("Hello World!");

            // Keep the console window open in debug mode.
            Console.WriteLine("Press any key to exit.");
            Console.ReadKey();
        }
    }
}
 
